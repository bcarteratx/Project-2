<%- include('../partials/header') %>

    <h4>Edit an Event</h4>
<form action="/events" method="POST">
  </div>
	<label>Type of Event:
	  <input type="text" value="<%= event.eventType %>" name="eventType">
	</label><br>
	<label>Location:
	  <input type="text" value="<%= event.location %>" name="location">
	</label><br>
	<label>Start time:
	  <input type="datetime-local" value="" name="startTime">
  </label><br>
  <label>Volunteers:
    <input type="text" value="<%= event.volunteers %>" name="volunteers">
      </label><br>
  <label>Trash Collected:
    <input type="text" value="<%= event.trashLbs %>" name="trashLbs">
      </label><br>
  <label>Miles Run:
    <input type="text" value="<%= event.miles %>" name="miles">
      </label><br>
  <label>Image file:
    <input type="text" value="<%= event.img %>" name="img">
      </label><br>
  <input type="submit" value="Add Changes">
</form>

<%- include('../partials/footer') %>

